# Environment
ENVIRONMENT=test
NODE_ENV=test

# Service hosts and ports
DOCKER_SERVICE_HOST=backend-test
DOCKER_SERVICE_PORT=8000
POSTGRES_HOST=db-test
POSTGRES_PORT=5432
REDIS_HOST=redis-test
REDIS_PORT=6379
REDIS_DB=0

# Authentication
JWT_SECRET_KEY=test_secret_key_123
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# WebSocket settings
WS_MAX_CONNECTIONS_PER_USER=5
WS_RATE_LIMIT=60
WS_PING_INTERVAL=20.0
WS_PING_TIMEOUT=20.0
WS_CLOSE_TIMEOUT=20.0
WS_MAX_HISTORY_SIZE=100
WS_MAX_MESSAGE_LENGTH=4096
WS_MAX_CONNECTIONS_PER_IP=20
WS_MAX_MESSAGES_PER_SECOND=10

# Database
POSTGRES_DB=test_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db-test:5432/test_db

# Redis
REDIS_MAX_CONNECTIONS=10
REDIS_TIMEOUT=5

# Testing
ENABLE_TELEMETRY=true
LOG_LEVEL=DEBUG

# MCP settings
MCP_HOST=backend-test
MCP_PORT=8000

# Real service test settings
MCP_API_KEY=${MCP_API_KEY}
MCP_USER_ID=${MCP_USER_ID}
MCP_USER_EMAIL=${MCP_USER_EMAIL}
MCP_USERNAME=${MCP_USERNAME} 